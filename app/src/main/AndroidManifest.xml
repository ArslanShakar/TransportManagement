<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.climesoftt.transportmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AllMechanicsActivity"
            android:label="@string/title_activity_all_mechanics"/>

        <activity android:name=".AllUsersActivity"
            android:label="@string/title_activity_all_users" />

        <activity android:name=".LoginActivity"
            android:label="@string/title_activity_login" />

        <activity android:name=".AdminDashboardActivity"
         android:label="@string/title_activity_admin_dashboard" />

        <activity android:name=".RegistrationActivity"
            android:label="@string/title_activity_resgistration"/>

        <activity android:name=".UserProfile"
            android:label="@string/title_activity_user_profile" />

        <activity android:name=".AllRoutes"
            android:label="@string/title_activity_all_routes" />
        <activity android:name=".AddRoute"
            android:label="@string/title_activity_add_route" />
    </application>

</manifest>